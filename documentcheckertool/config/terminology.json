        "max_paragraph_sentences": 5,
        "passive_voice_patterns": [
            "\\b(?:am|is|are|was|were|be|been|being)\\s+\\w+ed\\b",
            "\\b(?:am|is|are|was|were|be|been|being)\\s+\\w+en\\b",
